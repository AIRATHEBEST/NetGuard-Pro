# ============================================================
# NetGuardPro — Environment Variables
# Copy this file to .env and fill in your values.
# NEVER commit .env to version control.
# ============================================================

# ── Supabase ──────────────────────────────────────────────
# Found in: Supabase Dashboard → Project Settings → API
SUPABASE_URL=https://<your-project-ref>.supabase.co
SUPABASE_ANON_KEY=<your-anon-public-key>
SUPABASE_SERVICE_ROLE_KEY=<your-service-role-secret-key>

# DATABASE_URL is the direct PostgreSQL connection string.
# Found in: Supabase Dashboard → Project Settings → Database → Connection string (URI)
# Use the "Transaction" mode pooler URL for serverless/edge environments.
DATABASE_URL=postgresql://postgres:<password>@db.<project-ref>.supabase.co:5432/postgres

# ── App ───────────────────────────────────────────────────
NODE_ENV=development
PORT=3000

# ── Manus OAuth ───────────────────────────────────────────
VITE_APP_ID=
OAUTH_SERVER_URL=
OWNER_OPEN_ID=
JWT_SECRET=

# ── Manus Forge API (storage / LLM) ───────────────────────
BUILT_IN_FORGE_API_URL=
BUILT_IN_FORGE_API_KEY=
