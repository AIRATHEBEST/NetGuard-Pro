# ============================================================
# NetGuard Pro - Environment Variables
# Copy this file to .env and fill in your values
# ============================================================

# Supabase Configuration (required)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Vite client-side Supabase (required for frontend)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# App Configuration
NODE_ENV=development
PORT=3000
JWT_SECRET=change-this-to-a-secure-random-string

# Database (Direct PostgreSQL connection - optional, Supabase client is used by default)
DATABASE_URL=postgresql://postgres:your-password@db.your-project.supabase.co:5432/postgres

# Agent WebSocket Port (for network agents)
AGENT_WS_PORT=8080
